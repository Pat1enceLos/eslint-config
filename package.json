{
  "name": "@trevortan/eslint-config",
  "version": "0.0.2",
  "description": "ESLint config presets inspired by antfu",
  "main": "./dist/index.js",
  "homepage": "https://github.com/Pat1enceLos/eslint-config",
  "license": "MIT",
  "type":"module",
  "scripts": {
    "lint": "eslint ./fixture",
    "build": "tsup src/index.js --format esm,cjs --clean"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "eslint config"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "niceless0416@gmail.com",
  "devDependencies": {
    "eslint": "^8.22.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-n": "^16.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "tsup": "^7.2.0",
    "vue-eslint-parser": "^9.3.2"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  }
}
